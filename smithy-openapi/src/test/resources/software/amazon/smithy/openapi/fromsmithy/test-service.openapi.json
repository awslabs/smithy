{
  "openapi": "3.0.2",
  "info": {
    "title": "RestService",
    "version": "2006-03-01"
  },
  "paths": {
    "/document": {
      "post": {
        "operationId": "CreateDocument",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "abc": {
                    "type": "string",
                    "format": "password"
                  },
                  "def": {
                    "type": "number",
                    "format": "int32"
                  },
                  "hij": {
                    "type": "object",
                    "additionalProperties": {
                      "type": "string",
                      "format": "password"
                    }
                  },
                  "stringDateTime": {
                    "type": "string",
                    "format": "date-time"
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "Query list docs!",
            "style": "form",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "description": "Query list member reference docs!"
              }
            },
            "explode": true
          }
        ],
        "responses": {
          "200": {
            "description": "CreateDocument 200 response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "taggedUnion": {
                      "$ref": "#/components/schemas/ExampleRestCreateDocumentOutputTaggedUnionMember"
                    },
                    "foo": {
                      "type": "string"
                    },
                    "baz": {
                      "type": "string"
                    },
                    "time": {
                      "type": "number"
                    },
                    "list": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "maxItems": 10,
                      "minItems": 0
                    }
                  },
                  "required": [
                    "time"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/payload/{path}": {
      "put": {
        "operationId": "PutPayload",
        "requestBody": {
          "content": {
            "application/octet-stream": {
              "schema": {
                "type": "string",
                "format": "byte"
              }
            }
          }
        },
        "parameters": [
          {
            "name": "path",
            "in": "path",
            "schema": {
              "type": "string"
            },
            "required": true
          },
          {
            "name": "query",
            "in": "query",
            "schema": {
              "type": "number",
              "format": "int32"
            }
          },
          {
            "name": "timeQuery",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "X-Foo-Header",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-EnumString",
            "in": "header",
            "schema": {
              "type": "string",
              "enum": [
                "a",
                "c"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "PutPayload 200 response",
            "headers": {
              "X-Foo-Header": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "byte"
                }
              }
            }
          },
          "404": {
            "description": "PayloadNotFound 404 response",
            "headers": {
              "X-Foo-Header": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ExampleRestCreateDocumentOutputTaggedUnionMember": {
        "oneOf": [
          {
            "type": "object",
            "properties": {
              "a": {
                "type": "string"
              }
            },
            "required": [
              "a"
            ]
          },
          {
            "type": "object",
            "properties": {
              "b": {
                "type": "string"
              }
            },
            "required": [
              "b"
            ]
          }
        ]
      }
    }
  }
}
