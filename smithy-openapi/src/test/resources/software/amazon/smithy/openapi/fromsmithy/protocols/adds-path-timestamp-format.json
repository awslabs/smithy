{
  "smithy": "0.1.0",
  "smithy.example": {
    "shapes": {
      "Service": {
        "type": "service",
        "version": "2006-03-01",
        "protocols": [{"name": "aws.rest-json"}],
        "operations": [
          "Operation"
        ]
      },
      "Operation": {
        "type": "operation",
        "input": "OperationInput",
        "http": {
          "uri": "/{timestamp1}/{timestamp2}",
          "method": "POST"
        }
      },
      "OperationInput": {
        "type": "structure",
        "members": {
          "timestamp1": {
            "target": "Timestamp",
            "required": true,
            "httpLabel": true
          },
          "timestamp2": {
            "target": "Timestamp",
            "required": true,
            "httpLabel": true,
            "timestampFormat": "epoch-seconds"
          }
        }
      }
    }
  }
}
