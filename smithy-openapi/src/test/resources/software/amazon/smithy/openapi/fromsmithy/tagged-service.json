{
  "smithy": "0.1.0",
  "smithy.example": {
    "shapes": {
      "Service": {
        "type": "service",
        "version": "2006-03-01",
        "tags": ["foo"],
        "protocols": [{"name": "aws.rest-json"}],
        "operations": ["Operation1", "Operation2", "Operation3"]
      },
      "Operation1": {
        "type": "operation",
        "tags": ["bar", "baz", "foo", "qux"],
        "http": {"uri": "/operation1", "method": "GET"}
      },
      "Operation2": {
        "type": "operation",
        "tags": ["qux"],
        "http": {"uri": "/operation2", "method": "GET"}
      },
      "Operation3": {
        "type": "operation",
        "http": {"uri": "/operation3", "method": "GET"}
      }
    }
  }
}
