{
  "smithy": "1.0",
  "example.smithy": {
    "shapes": {
      "MyService": {
        "type": "service",
        "version": "2006-03-01",
        "protocols": {
          "aws.rest-json": {}
        },
        "operations": [
          "MyOperation"
        ],
        "aws.apigateway#apiKeySource": "HEADER"
      },
      "MyOperation": {
        "type": "operation",
        "input": "MyOperationInput",
        "http": {
          "uri": "/foo",
          "method": "GET"
        }
      },
      "MyOperationInput": {
        "type": "structure"
      }
    }
  }
}
