{
    "smithy": "0.5.0",
    "shapes": {
        "ns.foo#MyService": {
            "type": "service",
            "version": "2020-03-12",
            "operations": [
                {
                    "target": "ns.foo#Operation"
                },
                {
                    "target": "ns.foo#Operation2"
                }
            ]
        },
        "ns.foo#Operation": {
            "type": "operation",
            "input": {
                "target": "ns.foo#OperationInput"
            },
            "output": {
                "target": "ns.foo#OperationOutput"
            },
            "traits": {
                "smithy.api#readonly": true,
                "smithy.api#examples": [
                    {
                        "title": "Testing 1",
                        "input": {
                            "foo": "value1"
                        },
                        "output": {
                            "bam": "value2"
                        }
                    },
                    {
                        "title": "Testing 2",
                        "input": {
                            "baz": "baz"
                        },
                        "output": {
                            "additional": "value"
                        }
                    }
                ]
            }
        },
        "ns.foo#OperationInput": {
            "type": "structure",
            "members": {
                "foo": {
                    "target": "ns.foo#String",
                    "traits": {
                        "smithy.api#required": true
                    }
                },
                "baz": {
                    "target": "ns.foo#String"
                }
            }
        },
        "ns.foo#OperationOutput": {
            "type": "structure",
            "members": {
                "bam": {
                    "target": "ns.foo#String",
                    "traits": {
                        "smithy.api#required": true
                    }
                }
            }
        },
        "ns.foo#Operation2": {
            "type": "operation",
            "traits": {
                "smithy.api#readonly": true,
                "smithy.api#examples": [
                    {
                        "title": "Testing 3",
                        "input": {
                            "foo": "baz"
                        },
                        "output": {
                            "foo": "baz"
                        }
                    }
                ]
            }
        },
        "ns.foo#String": {
            "type": "string"
        }
    }
}
