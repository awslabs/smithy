{
  "smithy": "0.4.0",
  "example.smithy": {
    "shapes": {
      "EmptyOperation": {
        "type": "operation"
      },

      "NotEventStreamOperation": {
        "type": "operation",
        "input": "NotEventStreamStructure",
        "output": "NotEventStreamStructure"
      },
      "NotEventStreamStructure": {
        "type": "structure",
        "members": {
          "foo": {"target": "smithy.api#String"}
        }
      },

      "EventStreamOperation": {
        "type": "operation",
        "input": "EventStreamOperationInput",
        "output": "EventStreamOperationOutput"
      },

      "EventStreamOperationInput": {
        "type": "structure",
        "members": {
          "a": {"target": "smithy.api#String"},
          "b": {"target": "smithy.api#Integer"},
          "c": {"target": "InputEventStream", "eventStream": true}
        }
      },

      "EventStreamOperationOutput": {
        "type": "structure",
        "members": {
          "a": {"target": "smithy.api#String"},
          "b": {"target": "smithy.api#Integer"},
          "c": {"target": "OutputEventStream", "eventStream": true}
        }
      },

      "InputEventStream": {
        "type": "union",
        "members": {
          "eventA": {"target": "EventStructure"},
          "eventB": {"target": "EventStructure"}
        }
      },

      "OutputEventStream": {
        "type": "union",
        "members": {
          "eventA": {"target": "EventStructure"},
          "eventB": {"target": "EventStructure"}
        }
      },

      "EventStructure": {
        "type": "structure",
        "members": {
          "foo": {"target": "smithy.api#String"}
        }
      },

      "SingleEventOperation": {
        "type": "operation",
        "input": "SingleEventOperationInput"
      },

      "SingleEventOperationInput": {
        "type": "structure",
        "members": {
          "messages": {"target": "EventStructure", "eventStream": true}
        }
      }
    }
  }
}
