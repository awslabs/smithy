                    "a": {
                        "target": "MyString",
                        "required": true
                    },
                    "c": {
                        "target": "ns.shapes#String"
                    }
                },
                "documentation": "abc"
            },
            "BasicResource": {
                "type": "resource"
            },
            "b": {
                "type": "structure",
                "trait": true
            },
            "a": {
                "type": "structure",
                "trait": true
            },
            "OperationNeedingAbsoluteShapeIds": {
