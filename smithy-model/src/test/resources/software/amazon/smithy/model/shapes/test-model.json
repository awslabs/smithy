                "type": "operation",
                "idempotent": true,
                "input": "PutMyResourceInput"
            },
            "PutMyResourceInput": {
                "type": "structure",
                "members": {
                    "id": {
                        "target": "MyResourceId",
                        "required": true
                    }
                }
            },
            "ResourceNotFoundError": {
                "type": "structure",
                "error": "client"
            },
            "MyString": {
                "type": "string",
                "mediaType": "application\/json",
                "documentation": "docs"
            },
            "AList": {
                "type": "list",
                "documentation": "abc",
                "member": {
                    "target": "MyString"
                }
            },
            "ASet": {
                "type": "set",
                "documentation": "abc",
                "member": {
                    "target": "MyString"
                }
            },
            "Map": {
                "type": "map",
                "documentation": "abc",
