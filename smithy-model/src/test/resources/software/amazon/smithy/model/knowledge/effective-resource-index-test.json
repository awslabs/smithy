{
    "smithy": "1.0",
    "example.smithy": {
        "shapes": {
            "MyService": {
                "type": "service",
                "version": "2018-04-16",
                "operations": [
                    "SomeOperation"
                ],
                "resources": [
                    "SomeResource"
                ]
            },
            "SomeOperation": {
                "type": "operation",
                "readonly": true,
                "http": {
                    "uri": "\/",
                    "method": "POST"
                }
            },
            "CreateSomeResource": {
                "type": "operation",
                "idempotent": true,
                "input": "CreateSomeResourceInput",
                "http": {
                    "uri": "\/some-resource\/{bar}",
                    "method": "PUT"
                }
            },
            "CreateChildResource": {
                "type": "operation",
                "idempotent": true,
                "input": "CreateChildResourceInput",
                "http": {
                    "uri": "\/some-resource\/{bar}\/child-resource\/{baz}",
                    "method": "PUT"
                }
            },
            "ListSomeResources": {
                "type": "operation",
                "readonly": true,
                "http": {
                    "uri": "\/some-resource",
                    "method": "GET"
                }
            },
            "ListChildResources": {
                "type": "operation",
                "readonly": true,
                "input": "ListChildResourcesInput",
                "http": {
                    "uri": "\/some-resource\/{bar}\/child-resource",
                    "method": "GET"
                }
            },
            "SomeResource": {
                "type": "resource",
                "identifiers": {
                    "bar": "smithy.api#String"
                },
                "create": "CreateSomeResource",
                "list": "ListSomeResources",
                "resources": [
                    "ChildResource"
                ]
            },
            "ChildResource": {
                "type": "resource",
                "identifiers": {
                    "bar": "smithy.api#String",
                    "baz": "smithy.api#String"
                },
                "create": "CreateChildResource",
                "list": "ListChildResources"
            },
            "CreateSomeResourceInput": {
                "type": "structure",
                "members": {
                    "foo": {
                        "target": "smithy.api#String"
                    },
                    "bar": {
                        "target": "smithy.api#String",
                        "required": true,
                        "httpLabel": true
                    }
                }
            },
            "CreateChildResourceInput": {
                "type": "structure",
                "members": {
                    "foo": {
                        "target": "smithy.api#String"
                    },
                    "bar": {
                        "target": "smithy.api#String",
                        "required": true,
                        "httpLabel": true
                    },
                    "baz": {
                        "target": "smithy.api#String",
                        "required": true,
                        "httpLabel": true
                    }
                }
            },
            "ListChildResourcesInput": {
                "type": "structure",
                "members": {
                    "bar": {
                        "target": "smithy.api#String",
                        "required": true,
                        "httpLabel": true
                    }
                }
            },
            "RootOperationOutput": {
                "type": "structure",
                "members": {
                    "abc": {
                        "target": "smithy.api#String"
                    }
                }
            }
        }
    }
}
