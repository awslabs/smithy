{
    "smithy": "0.1.0",
    "ns.foo": {
        "shapes": {
            "MyService": {
                "type": "service",
                "version": "2017-01-17",
                "protocols": [{"name": "foo", "auth": ["foo.baz", "foo.bar"]}],
                "operations": [
                    "MyOperation"
                ],
                "resources": [
                    "MyResource"
                ]
            },
            "MyOperation": {
                "type": "operation",
                "readonly": true,
                "input": "MyOperationInput",
                "output": "MyOperationOutput",
                "errors": [
                    "MyOperationError"
                ]
            },
            "MyOperationInput": {
                "type": "structure"
            },
            "MyOperationOutput": {
                "type": "structure",
                "references": {
                    "myResource": {
                        "resource": "MyResource",
                        "ids": {
                            "id": "id"
                        }
                    }
                },
                "members": {
                    "id": {
                        "target": "MyResourceIdentifier",
                        "required": true
                    }
                }
            },
            "MyOperationError": {
                "type": "structure",
                "error": "client"
            },
            "MyResource": {
                "type": "resource",
                "identifiers": {
                    "id": "MyResourceIdentifier"
                },
                "create": "CreateMyResource",
                "operations": [
                    "MyResourceOperation"
                ]
            },
            "MyResourceIdentifier": {
                "type": "string"
            },
            "CreateMyResource": {
                "type": "operation",
                "collection": true,
                "output": "CreateMyResourceOutput"
            },
            "CreateMyResourceOutput": {
                "type": "structure",
                "members": {
                    "id": {
                        "target": "MyResourceIdentifier"
                    }
                }
            },
            "MyResourceOperation": {
                "type": "operation",
                "readonly": true,
                "input": "MyResourceOperationInput"
            },
            "MyResourceOperationInput": {
                "type": "structure",
                "members": {
                    "id": {
                        "target": "MyResourceIdentifier",
                        "required": true
                    },
                    "otherValue": {
                        "target": "MyResourceOperationInputString"
                    }
                }
            },
            "MyResourceOperationInputString": {
                "type": "string",
                "tags": [
                    "foo"
                ]
            }
        }
    }
}
