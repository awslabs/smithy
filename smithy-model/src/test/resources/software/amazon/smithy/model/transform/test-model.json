{
    "smithy": "1.0",
    "ns.foo": {
        "shapes": {
            "MyService": {
                "type": "service",
                "version": "2017-01-17",
                "protocols": [{"name": "foo"}],
                "operations": [
                    "MyOperation"
                ]
            },
            "MyOperation": {
                "type": "operation",
                "readonly": true,
                "input": "MyOperationInput",
                "output": "MyOperationOutput",
                "errors": [
                    "MyOperationError"
                ]
            },
            "MyOperationInput": {
                "type": "structure"
            },
            "MyOperationOutput": {
                "type": "structure"
            },
            "MyOperationError": {
                "type": "structure",
                "error": "client"
            }
        }
    }
}
