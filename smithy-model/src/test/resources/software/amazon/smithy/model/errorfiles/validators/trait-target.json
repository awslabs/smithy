{
    "smithy": "0.5.0",
    "shapes": {
        "ns.foo#String": {
            "type": "string",
            "traits": {
                "smithy.api#range": {
                    "min": 0,
                    "max": 10
                }
            }
        },
        "ns.foo#Invoke": {
            "type": "operation",
            "input": {
                "$target": "ns.foo#InvokeInput"
            }
        },
        "ns.foo#InvokeInput": {
            "type": "structure",
            "members": {
                "foo": {
                    "$target": "ns.foo#String",
                    "traits": {
                        "ns.foo#test": true
                    }
                }
            }
        },
        "ns.foo#Invalid": {
            "type": "structure",
            "members": {
                "baz": {
                    "$target": "ns.foo#String",
                    "traits": {
                        "ns.foo#test": true
                    }
                }
            }
        },
        "ns.foo#test": {
            "type": "structure",
            "traits": {
                "smithy.api#trait": {
                    "selector": "operation -[input]-> structure > member"
                }
            }
        }
    }
}
