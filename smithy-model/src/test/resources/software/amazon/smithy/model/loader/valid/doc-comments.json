{
  "smithy": "0.3.0",
  "smithy.example": {
    "shapes": {
      "MyString": {
        "type": "string",
        "documentation": "Foo\nbaz\nBar!"
      },
      "MyStructure": {
        "type": "structure",
        "documentation": "Structure",
        "members": {
          "foo": {"target": "String", "documentation": "Docs on member!"},
          "baz": {"target": "String", "documentation": "Docs on another member!"}
        }
      },
      "MyTrait": {
        "type": "structure",
        "documentation": "The documentation\nof this trait!",
        "trait": {}
      }
    }
  }
}
