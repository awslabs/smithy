{
    "smithy": "0.5.0",
    "shapes": {
        "ns.foo#FooService": {
            "type": "service",
            "version": "2020-03-12",
            "operations": [
                {
                    "target": "ns.foo#Valid"
                },
                {
                    "target": "ns.foo#Invalid"
                }
            ]
        },
        "ns.foo#Valid": {
            "type": "operation",
            "input": {
                "target": "ns.foo#ValidInput"
            }
        },
        "ns.foo#ValidInput": {
            "type": "structure",
            "members": {
                "token": {
                    "target": "ns.foo#String",
                    "traits": {
                        "smithy.api#idempotencyToken": true
                    }
                }
            }
        },
        "ns.foo#String": {
            "type": "string"
        },
        "ns.foo#Invalid": {
            "type": "operation",
            "input": {
                "target": "ns.foo#InvalidInputWithMultiple"
            },
            "traits": {
                "smithy.api#readonly": true
            }
        },
        "ns.foo#InvalidInputWithMultiple": {
            "type": "structure",
            "members": {
                "token": {
                    "target": "ns.foo#String",
                    "traits": {
                        "smithy.api#idempotencyToken": true
                    }
                },
                "anotherToken": {
                    "target": "ns.foo#String",
                    "traits": {
                        "smithy.api#idempotencyToken": true
                    }
                }
            }
        }
    }
}
